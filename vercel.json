{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "API_KEY": "@api_key",
    "SIGNING_SECRET": "@signing_secret"
  }
}
